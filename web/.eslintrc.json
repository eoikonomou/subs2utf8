{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "parser": "babel-eslint",
    "rules": {
        "import/no-named-as-default-member": "off",
        "import/no-named-as-default": "off",
        "import/no-unresolved": "off",
        "react/forbid-prop-types": "off",
        "no-console": "off",
        "semi": "error"
    }
}
